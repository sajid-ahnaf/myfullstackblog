{
  "name": "w3schools-blog-nodejs-template",
  "version": "1.0.0",
  "description": "A simple blog app built in W3Schools Spaces",
  "main": "app.js",
  "scripts": {
    "start": "webpack --mode none && nodemon app.js",
    "build": "webpack --mode none",
    "seed-articles": "node seed-articles.js",
    "seed-blank": "node seed-blank.js",
    "dev": "webpack --mode development 2>&1 & nodemon -I -q app.js",
    "admin:createuser": "run-func utilities/admin-utilities.js createAdminUser",
    "admin:changeuserpassword": "run-func utilities/admin-utilities.js changeAdminUserPassword",
    "admin:deleteuser": "run-func utilities/admin-utilities.js deleteAdminUser"
  },
  "keywords": [
    "express",
    "node",
    "w3schools",
    "spaces",
    "blog"
  ],
  "dependencies": {
    "@ckeditor/ckeditor5-alignment": "^35.4.0",
    "@ckeditor/ckeditor5-basic-styles": "^35.4.0",
    "@ckeditor/ckeditor5-clipboard": "^35.4.0",
    "@ckeditor/ckeditor5-core": "^35.4.0",
    "@ckeditor/ckeditor5-dev-utils": "^31.1.13",
    "@ckeditor/ckeditor5-engine": "^35.4.0",
    "@ckeditor/ckeditor5-essentials": "^35.4.0",
    "@ckeditor/ckeditor5-font": "^35.4.0",
    "@ckeditor/ckeditor5-image": "^35.4.0",
    "@ckeditor/ckeditor5-paragraph": "^35.4.0",
    "@ckeditor/ckeditor5-theme-lark": "^35.4.0",
    "@ckeditor/ckeditor5-ui": "^35.4.0",
    "@ckeditor/ckeditor5-upload": "^35.4.0",
    "@ckeditor/ckeditor5-utils": "^35.4.0",
    "argon2": "^0.30.2",
    "axios": "^1.2.1",
    "better-sqlite3": "^7.6.2",
    "ckeditor5": "^35.4.0",
    "cookie-parser": "^1.4.6",
    "css-loader": "^5.2.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-handlebars": "^6.0.6",
    "handlebars-dateformat": "^1.1.3",
    "html-to-text": "^8.2.1",
    "jsdom": "^20.0.3",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.20",
    "postcss-loader": "^4.3.0",
    "prompt-sync": "^4.2.0",
    "raw-loader": "^4.0.2",
    "run-func": "^3.0.0",
    "sqlite3": "^5.1.2",
    "style-loader": "^2.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "ws": "^8.11.0"
  }
}
